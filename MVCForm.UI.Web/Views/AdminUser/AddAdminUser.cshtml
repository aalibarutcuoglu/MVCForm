
@{
    Layout = null;
}
@model MVCForm.UI.Web.Models.VM.AdminUserVM
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>AddAdminUser</title>
</head>
<body>
    <div>
        @Html.ValidationSummary()
        <form action="/AdminUser/AddAdminUser" method="post">
            <div>
                @Html.LabelFor(q => q.Username)
